<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Gallery</title>
</head>
<body>
    <div style="text-align: center;" class="image-container">
        <% images.forEach(image => { %>
            <img style="width: 300px; height: 300px;" src="/uploads/<%= image %>" alt="<%= image %>">
        <% }) %>
    </div>
</body>
</html>
